{
  "name": "@rineex/root",
  "version": "1.0.0",
  "description": "Root package for Rineex core modules",
  "main": "index.js",
  "scripts": {
    "changeset": "changeset",
    "v": "changeset version",
    "release": "changeset publish"
  },
  "keywords": [
    "rineex",
    "core",
    "packages"
  ],
  "author": "Rineex Team",
  "license": "Apache-2.0",
  "packageManager": "pnpm@10.22.0",
  "devDependencies": {
    "@fullstacksjs/eslint-config": "13.11.2",
    "@vitest/ui": "4.0.16",
    "cspell": "9.4.0",
    "eslint": "9.39.2",
    "lint-staged": "16.2.7",
    "prettier": "3.7.4",
    "simple-git-hooks": "2.13.1",
    "turbo": "2.6.3",
    "vitest": "4.0.16"
  },
  "volta": {
    "node": "24.12.0"
  },
  "lint-staged": {
    "*.{md,json,yml,yaml,html}": [
      "prettier --write",
      "cspell --no-must-find-files"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "cspell --no-must-find-files"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "lint-staged",
    "pre-push": "npm run format",
    "preserveUnused": true
  }
}
