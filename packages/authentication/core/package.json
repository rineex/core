{
  "name": "@rineex/auth-core",
  "version": "0.1.0",
  "description": "Authentication Core package for Rineex core modules",
  "author": "Rineex Team",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest --watch --passWithNoTests",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "check-types": "tsc --noEmit",
    "build": "tsup"
  },
  "keywords": [
    "rineex",
    "authentication",
    "auth",
    "social-login",
    "otp",
    "passwordless",
    "sso"
  ],
  "files": [
    "dist/**"
  ],
  "publishConfig": {
    "provenance": true,
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@changesets/cli": "2.29.8",
    "@rineex/eslint-config": "workspace:*",
    "@rineex/typescript-config": "workspace:*",
    "@types/lodash.isempty": "4.4.9",
    "@types/lodash.isnil": "4.0.9",
    "@types/node": "24.10.4",
    "@vitest/ui": "4.0.16",
    "tslib": "2.8.1",
    "tsup": "8.5.1",
    "type-fest": "5.3.1",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "6.0.2",
    "vitest": "4.0.16"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rineex/core.git",
    "directory": "packages/authentication/core"
  },
  "homepage": "https://github.com/rineex/core/tree/main/packages/authentication/core",
  "license": "Apache-2.0",
  "packageManager": "pnpm@10.22.0",
  "dependencies": {
    "@rineex/ddd": "workspace:*",
    "lodash.isempty": "4.4.0",
    "lodash.isnil": "4.0.0"
  }
}
